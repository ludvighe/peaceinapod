    {
      "name": "peaceinapod (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "peaceinapod (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
